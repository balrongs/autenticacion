#%RAML 1.0 DataType
type: object
properties:
  first_name?:
    type: string
    minLength: 3
    maxLength: 80
  last_name?:
    type: string
    minLength: 3
    maxLength: 80
  email?:
    type: string
    maxLength: 150
    description: Must be unique.
examples:
  -
    first_name: 'Marcelo'
    last_name: 'Fuenzalida'
    email: 'm.fuenzalida@example.com'
