  "dependencies": {
    "@koa/cors": "^2.2.3",
    "aws-sdk": "^2.437.0",
    "bcrypt": "^3.0.4",
    "fastest-validator": "^0.6.13",
    "jsonwebtoken": "^8.4.0",
    "koa": "^2.6.2",
    "koa-body": "^4.0.8",
    "koa-json": "^2.0.2",
    "koa-router": "^7.4.0",
    "nodemailer": "^6.4.16",
    "pg": "^7.8.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.42.0",
